/**
 * This class tests a bunch of different member declarations
 */
class Member {

	//Public
    get int a;   	// basic int
    get double b;	// basic double
    set float c;		// basic float
    set code d;   	// basic code
    get set byte e;		// basic byte
    get set boolean f;	// basic boolean
    get set short g;		// basic short
    get set long h;		// basic short    
    get set Object i = Object:create();	// basic object
    
	//Private
    int j;   	// private int
    double k;	// private double
    float l;	// private float
    code m;   	// private code
    byte n;		// private byte
    boolean o;	// private boolean
    short p;	// private short
    long q;		// private short    
    Object s = Object:create();		// private object    


  	//Modifiers
    get set int t;   		// basic int
    get int u;   			// readable int
    set nullable int v;		// nullable settable int
    immutable int w = 3;	// immutable int
    get nullable double x;	// nullable gettable double
}
