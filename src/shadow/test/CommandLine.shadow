import shadow:io@Console;

class shadow:test@CommandLine
{
	public main( String[] args ) => ()
	{
		String value = "bleh";
		String word = "bleh";
		Console out;
		
		if( value == word )
			out.printLine("Yes!");
		
		for( int i = args->size - 1; i >= 0; i -= 1 )
		{
			value = args[i];
			out.print(value);
			
			if( value == word )
				out.printLine(" matches!");
			else
				out.printLine(" doesn't match!");
		}		
	}
}
